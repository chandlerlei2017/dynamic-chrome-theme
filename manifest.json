{
  "name": "Dynamic Chrome Themes",
  "version": "1.0",
  "description": "Dynamic Chrome themes",
  "permissions": ["activeTab","declarativeContent", "storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}